{
  "name": "gpt4-docs-chatbot",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Inflaton",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-netlify": "cp netlify/package.json . && yarn build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx)\" --write",
    "test": "tsx -r dotenv/config scripts/question_answer.ts",
    "pinecone": "yarn build && yarn start | tee data/logs/pinecone.log",
    "hnswlib": "yarn build && yarn start | tee data/logs/hnswlib.log",
    "chroma": "yarn build && yarn start | tee data/logs/chroma.log",
    "milvus": "yarn build && yarn start | tee data/logs/milvus.log",
    "load": "tsx -r dotenv/config scripts/load-data.ts"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "@pinecone-database/pinecone": "^1.1.2",
    "@radix-ui/react-accordion": "^1.1.1",
    "@zilliz/milvus2-sdk-node": "^2.2.7",
    "chromadb": "^1.4.1",
    "clsx": "^1.2.1",
    "cohere-ai": "^6.2.0",
    "dotenv": "^16.0.3",
    "encoding": "^0.1.13",
    "hnswlib-node": "^1.4.2",
    "langchain": "^0.0.104",
    "lucide-react": "^0.125.0",
    "next": "13.4.7",
    "pdf-parse": "1.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.5",
    "sharp": "^0.32.1",
    "tailwind-merge": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/parser": "^5.54.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18.x"
  },
  "keywords": [
    "starter",
    "gpt4",
    "pinecone",
    "chromadb",
    "milvus2",
    "hnswlib",
    "typescript",
    "nextjs",
    "langchain",
    "pdf",
    "html",
    "openai"
  ]
}